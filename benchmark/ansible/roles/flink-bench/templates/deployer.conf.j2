deployer {
  remote {
    manager-hostname = {{ groups['manager'][0] }}
    manager-port = 6123
    uber-jar = "{{ flink_dir }}/flink-benchmark-1.0-SNAPSHOT-uber.jar"
  }
  parallelism = {{ groups['workers'] | length }}
  rocksdb-path = "{{ flink_dir }}/data"
  guarantees = "{{ guarantees }}"
  millis-between-commits = {{ millis_between_commits }}
}
